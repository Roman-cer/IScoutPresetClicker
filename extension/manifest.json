{
  "manifest_version": 3,
  "name": "Button Cycle Clicker",
  "version": "1.0",
  "permissions": [ "storage", "tabs", "activeTab"],
  "content_scripts": [
    {
      "matches": [
        "https://*.iscout.club/*"
      ],
      "js": [ "content.js" ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}